{
  "name": "vesan",
  "version": "3.0.0-alpha",
  "description": "Frontend Framework with Vue, TypeScript, Vuex, Axios, Sass & Animadio",
  "type": "module",
  "keywords": [
    "axios",
    "blog",
    "fontawesome",
    "framework",
    "javascript",
    "paypal",
    "pinia",
    "recaptcha",
    "sass",
    "scss",
    "sfc",
    "shop",
    "tinymce",
    "typescript",
    "ui",
    "vitest",
    "vue",
    "vuex"
  ],
  "scripts": {
    "start": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --coverage",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@paypal/paypal-js": "^5.1.4",
    "@tinymce/tinymce-vue": "^5.1.1",
    "animadio": "^6.2.7",
    "axios": "^1.6.2",
    "pinia": "^2.1.7",
    "sass": "^1.32.7",
    "vue": "^3.4.21",
    "vue-recaptcha": "^2.0.3",
    "vue-router": "^4.2.5",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.10",
    "@types/vue": "^2.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.6.3",
    "eslint": "^8.49.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  },
  "homepage": "https://www.npmjs.com/package/vesan",
  "repository": "github:philippebeck/vesan",
  "bugs": "https://github.com/philippebeck/vesan/issues",
  "license": "Apache-2.0 License",
  "author": {
    "name": "Philippe Beck",
    "email": "philippe@philippebeck.net",
    "url": "https://philippebeck.net"
  }
}
